 return <div key={result.pageid} className=\"item\">\n            <div className=\"right floated content\">\n                <a\n                    href={`https://en.wikipedia.org?curid=${result.pageid}`}\n                    className=\"ui button\"\n                >\n                    Go\n                </a>\n            </div>\n            <div className=\"content\">\n                <div className=\"header\">\n                    {result.title}\n                </div>\n                <span dangerouslySetInnerHTML={{ __html: result.snippet }}></span>\n            </div>\n        </div>\n    });\n\n    return (\n        <div>\n            <div className=\"ui form\">\n                <div className=\"field\">\n                    <label>Enter Search Term</label>\n                    <input\n                        value={term}\n                        onChange={e => setTerm(e.target.value)}\n                        className=\"input\" />\n                </div>\n            </div>\n            <div className=\"ui celled list\">\n                {renderedResults}\n            </div>\n        </div>\n    );\n}\n\nexport default Search;","/home/illuminati/Documents/react/v2/widgets/src/componets/Dropdown.js",[],"/home/illuminati/Documents/react/v2/widgets/src/componets/Transalate.js",[],"/home/illuminati/Documents/react/v2/widgets/src/componets/Convert.js",["46"],"/home/illuminati/Documents/react/v2/widgets/src/componets/Route.js",[],"/home/illuminati/Documents/react/v2/widgets/src/componets/header.js",[],"/home/illuminati/Documents/react/v2/widgets/src/componets/Link.js",[],{"ruleId":"47","severity":1,"message":"48","line":39,"column":1,"nodeType":"49","endLine":64,"endColumn":2},{"ruleId":"50","severity":1,"message":"51","line":40,"column":8,"nodeType":"52","endLine":40,"endColumn":14,"suggestions":"53"},{"ruleId":"54","severity":1,"message":"55","line":3,"column":8,"nodeType":"56","messageId":"57","endLine":3,"endColumn":18},"import/no-anonymous-default-export